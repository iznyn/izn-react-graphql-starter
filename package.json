    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "graphiql": "NODE_ENV=development webpack-dev-server --config internals/webpack/webpack.graphiql.babel.js --content-base build/ --mode development --compress --port 4000 --inline --hot --colors --progress --history-api-fallback"
